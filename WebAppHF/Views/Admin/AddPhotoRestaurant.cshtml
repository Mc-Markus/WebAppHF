@model WebAppHF.Models.Restaurant

@{
    ViewBag.Title = "AddPhotoRestaurant";
}

@using (Html.BeginForm("AddPhotoRestaurant", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
{

    <div>
        <label for="file">Upload Image:</label>
        <input type="file" name="file" id="file" style="width: 100%;" />
        <input type="submit" value="Create" class="submit" />
    </div>
}

<h2>AddPhotoRestaurant</h2>

