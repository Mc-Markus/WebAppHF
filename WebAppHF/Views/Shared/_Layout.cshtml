<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <!-- Navigation -->
    <nav class="navbar fixed-top navbar-expand-lg navbar-light fixed-top navbar-styling">
        <div class="container">
            <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link" href="index.html">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="jazz.html">JazzPatronaat</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="dinner.html">Dinner Haarlem</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="talking.html">Talking Haarlem</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="historic.html">Historic Haarlem</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="contact.html">Contact</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="contact.html">Basket</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    <div class="carousel-holder">
        <div class="carousel slide" data-ride="carousel">
            <div class="carousel-inner" role="listbox">
                <div class="carousel-item active">
                    <img class="img-fluid carousel-img" src="../images/haarlem1.jpg">
                </div>
                <div class="carousel-item">
                    <img class="img-fluid carousel-img" src="images/haarlem2.jpg">
                </div>
                <div class="carousel-item">
                    <img class="img-fluid carousel-img" src="images/haarlem3.jpg">
                </div>
            </div>
        </div>
    </div>
    @RenderBody()
    <hr />
    <footer>
        <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
    </footer>
    <div class="container footer-container">
        <div class="row">
            <div class="col-md-3">
                <h2>Navigation</h2>
                <ul class="footer-list">
                    <li><a href="index.html">Home</a></li>
                    <li><a href="jazz.html">Jazz  Patronaat</a></li>
                    <li><a href="talking.html">Talking Haarlem</a></li>
                    <li><a href="dinner.html">Dinner Haarlem</a></li>
                    <li><a href="historic.html">Histroic Haarlem</a></li>
                    <li><a href="contact.html">Contact</a></li>
                    <li><a href="admin.html">Admin</a></li>
                </ul>
            </div>
            <div class="col-md-3">
                <h2>Contact</h2>
                <address>
                    <a href="mailto:webmaster@example.com">Email.nl</a>
                    <br>3100000000<br>
                    <br>
                    Head Office<br>
                    Box 564, Disneyland<br>
                    USA
                </address>
            </div>
            <div class="col-md-6">
                <h2>Location</h2>
                <iframe class="img-responsive" id="google-iframe" frameborder="0" style="border:0" src="https://www.google.com/maps/embed/v1/place?key=AIzaSyActajOoA4V5lSdaQtAsq2NtTKnqkcxZGs
                    &q=Space+Needle,Seattle+WA" allowfullscreen></iframe>
            </div>
        </div>
    </div>


    <!-- Marketing Icons Section -->
    <!-- /.container -->
    <!-- Footer -->
    <!-- Bootstrap core JavaScript -->
    <!--
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    -->

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
